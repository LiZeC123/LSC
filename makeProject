#! /bin/bash

if [ $1x == "ass"x ]
then
	cd ./ass
    make
elif [ $1x == "compile"x ]
then
	cd ./compile
    make
elif [ $1x == "lit"x ]
then
	cd ./lit
    make
elif [ $1x == "clear"x ]
then
	cd ./compile
	make clear
	cd ../ass
	make clear
else
	cd ./compile
	make -j8
	cd ../ass
	make -j8
	cd ../lit
	make -j8
fi