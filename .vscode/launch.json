        {
            "name": "链接器模块",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "makeLit",
            "program": "${workspaceFolder}/lit/lscl",
            "args": [
                "/usr/include/lsc/start.o",
                "/usr/include/lsc/lscio.o",
