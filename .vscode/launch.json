        {
            "name": "汇编器模块",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "makeAss",
            "program": "${workspaceFolder}/ass/lsca",
            "args": [
                "${workspaceFolder}/test/99test.s"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
